{
  "name": "h5-mobile-bank-vue",
  "version": "1.0.0",
  "description": "a mobile bank project develop of vue",
  "main": "index.js",
  "scripts": {
    "dev:single": "npm run serve & cli-dev-s --framework=react",
    "dev:multiple": "npm run serve & cli-dev-m --framework=react",
    "build:s-test": "npm run clean && cli-build-s --env=sit --framework=react all",
    "build:m-test": "npm run clean && cli-build-m --env=sit --framework=react all",
    "create": "cli-create --framework=react",
    "report": "webpack-bundle-analyzer ./dist/stats.json",
    "serve": "node ./json_server/server.js",
    "clean": "rm -rf dist",
    "prepare": "git init && chmod ug+x .husky/* && husky install",
    "pre-commit": "lint-staged",
    "test": "jest --coverage"
  },
  "author": "dreamjser@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@dreamjser/h5-webpack-cli": "^1.0.34",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.8",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "json-server": "^0.17.4",
    "lint-staged": "^13.3.0",
    "ts-jest": "^29.1.1",
    "webpack-bundle-analyzer": "^4.9.1"
  },
  "dependencies": {
    "@dreamjser/h5-loading-toast": "^1.0.3",
    "@dreamjser/request-axios": "^1.0.9",
    "@reduxjs/toolkit": "^1.9.5",
    "@swc/core": "^1.3.95",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "antd": "^5.7.3",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.14.2",
    "redux": "^4.2.1",
    "use-immer": "^0.9.0"
  }
}
